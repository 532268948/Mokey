<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.zust.module_music">

    <uses-permission android:name="android.permission.WAKE_LOCK"/>

    <application
        android:name=".app.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/main_tab_btn_habit_pressed"
        android:label="@string/app_name"
        android:roundIcon="@drawable/main_tab_btn_habit_pressed"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".container.ContainerActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.play.MusicPlayActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".ui.play.StoryPlayActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".ui.select.MusicSelectActivity"
                  android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>