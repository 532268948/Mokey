<?xml version="1.0" encoding="utf-8"?>
<com.example.lib_common.base.view.SquareFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/iv_picture"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <TextView
        android:id="@+id/tv_more"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/nine_picture_item_bg"
        android:gravity="center"
        android:text="@string/nine_picture_more"
        android:visibility="gone"
        tools:visibility="visible" />

</com.example.lib_common.base.view.SquareFrameLayout>
