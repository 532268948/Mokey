<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.example.module_report">

    <application
        android:name=".app.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/main_tab_btn_habit_pressed"
        android:label="@string/app_name"
        android:roundIcon="@drawable/main_tab_btn_habit_pressed"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:name">
        <activity android:name=".container.ContainerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.ReportDetailActivity"></activity>
    </application>

</manifest>